{
  "name": "WHO",
  "title": "WHO",
  "type": "cookbook",
  "columns": [
    "#date+year",
    "#indicator+code",
    "#indicator+value+num",
    "#sex+code"
  ],
  "recipes": [
    {
      "name": "Infant Mortality Rate",
      "title": "Infant Mortality Rate",
      "description": "Probability of dying between birth and age 1 per 1000 live births",
      "type": "timeseries",
      "ingredients": {
        "aggregateFunctions": [
          "sum"
        ],
        "filtersWith": [
        {
          "#indicator+code": "MDG_0000000001"
        }
        ],
        "aggregateColumns": [
          "#sex+code",
          "#date+year"
        ],
        "valueColumns": [
          "#indicator+value+num"
        ]
      }
    },
    {
      "name": "Life Expectancy",
      "title": "Life Expectancy at Birth",
      "description": "",
      "type": "timeseries",
      "ingredients": {
        "aggregateFunctions": [
          "sum"
        ],
        "filtersWith": [
        {
          "#indicator+code": "WHOSIS_000001"
        }
        ],
        "aggregateColumns": [
          "#sex+code",
          "#date+year"
        ],
        "valueColumns": [
          "#indicator+value+num"
        ]
      }
    },
    {
      "name": "Gov Health Expenditure",
      "title": "Per Capita Government Health Expenditure",
      "description": "Per capita government expenditure on health at average exchange rate",
      "type": "timeseries",
      "ingredients": {
        "aggregateFunctions": [
          "sum"
        ],
        "filtersWith": [
        {
          "#indicator+code": "WHS7_104"
        }
        ],
        "aggregateColumns": [
          "#date+year"
        ],
        "valueColumns": [
          "#indicator+value+num"
        ]
      }
    }
  ]
}
