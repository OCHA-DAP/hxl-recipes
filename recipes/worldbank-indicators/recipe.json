{
  "name": "worldbank-indicators",
  "title": "Worldbank Indicators",
  "type": "cookbook",
  "columns": [
    "#country+name",
    "#country+code",
    "#date+year",
    "#indicator+name",
    "#indicator+code",
    "#indicator+num"
  ],
  "recipes": [
    {
      "name": "worldbank-indicators-population-by-year",
      "title": "Total Population By Year",
      "description": "",
      "type": "timeseries",
      "ingredients": {
        "aggregateFunctions": [
          "sum"
        ],
        "aggregateColumns": [
          "#date+year"
        ],
        "filtersWith": [
          {
            "#indicator+name": "Population, total"
          }
        ],
        "valueColumns": [
          "#indicator+num"
        ]
      }
    },
    {
      "name": "worldbank-indicators-infant-deaths-by-year",
      "title": "Number of Infant Deaths By Year",
      "description": "",
      "type": "timeseries",
      "ingredients": {
        "aggregateFunctions": [
          "sum"
        ],
        "aggregateColumns": [
          "date+year"
        ],
        "filtersWith": [
          {
            "#indicator+name": "Number of infant deaths"
          }
        ],
        "valueColumns": [
          "#indicator+num"
        ]
      }
    },
    {
      "name": "worldbank-indicators-poverty-headcount-ratio",
      "title": "Percentage of Population in Poverty",
      "description": "Percentage of the population living on less than $1.90 a day at 2011 international prices.",
      "type": "timeseries",
      "ingredients": {
        "aggregateFunctions": [
          "sum"
        ],
        "aggregateColumns": [
          "#date+year"
        ],
        "filtersWith": [
          {
            "#indicator+name": "Poverty headcount ratio at $1.90 a day (2011 PPP) (% of population)"
          }
        ],
        "valueColumns": [
          "#indicator+num"
        ]
      }
    }
  ]
}